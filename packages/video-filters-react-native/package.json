{
  "name": "@stream-io/video-filters-react-native",
  "version": "0.2.8",
  "description": "Video Filters for the Stream React Native Video SDK",
  "packageManager": "yarn@3.5.0",
  "main": "dist/commonjs/index.js",
  "module": "dist/module/index.js",
  "types": "dist/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "dist",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "clean": "rimraf android/build && rimraf dist",
    "build": "yarn clean && bob build"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GetStream/stream-video-js.git"
  },
  "author": "https://getstream.io <support@getstream.io> (https://github.com/GetStream)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GetStream/stream-video-js/issues"
  },
  "homepage": "https://github.com/GetStream/stream-video-js#readme",
  "devDependencies": {
    "@stream-io/react-native-webrtc": "^125.0.7",
    "react-native": "0.74.1",
    "react-native-builder-bob": "^0.37.0",
    "rimraf": "^5.0.7",
    "typescript": "^5.5.2"
  },
  "peerDependencies": {
    "@stream-io/react-native-webrtc": ">=118.1.0",
    "react-native": "*"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
