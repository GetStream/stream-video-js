{
  "plugin": ["typedoc-plugin-markdown"],
  "entryPoints": ["./src/components", "./src/contexts", "./src/hooks"],
  "externalPattern": ["**/node_modules/**", "**/dist/**"],
  "excludeExternals": true,
  "cleanOutputDir": true,
  "excludePrivate": true,
  "out": "temp-docs",
  "logLevel": "Error",
  "tsconfig": "./tsconfig.json"
}
