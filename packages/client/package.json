{
  "name": "@stream-io/video-client",
  "version": "0.3.13",
  "packageManager": "yarn@3.2.4",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "browser": "dist/index.browser.es.js",
  "react-native": "dist/index.browser.es.js",
  "types": "dist/index.d.ts",
  "license": "See license in LICENSE",
  "scripts": {
    "postinstall": "yarn copy-version",
    "clean": "rimraf dist",
    "start": "yarn copy-version && rollup -w -c",
    "build": "yarn clean && yarn copy-version && rollup -c",
    "test": "vitest",
    "clean:docs": "rimraf generated-docs",
    "test-ci": "vitest --coverage",
    "generate:open-api": "./generate-openapi.sh protocol",
    "generate:open-api:dev": "./generate-openapi.sh chat",
    "copy-version": "echo \"export const version = '$npm_package_version';\" > ./version.ts"
  },
  "files": [
    "dist",
    "src",
    "index.ts",
    "package.json",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "@protobuf-ts/runtime": "^2.8.3",
    "@protobuf-ts/runtime-rpc": "^2.8.3",
    "@protobuf-ts/twirp-transport": "^2.8.3",
    "axios": "^1.4.0",
    "base64-js": "^1.5.1",
    "isomorphic-ws": "^5.0.0",
    "jsonwebtoken": "^9.0.0",
    "rxjs": "~7.8.1",
    "sdp-transform": "^2.14.1",
    "ua-parser-js": "^1.0.35",
    "webrtc-adapter": "^8.2.2",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/rimraf": "^3.0.2",
    "@types/sdp-transform": "^2.4.6",
    "@types/ua-parser-js": "^0.7.36",
    "@types/ws": "^8.5.4",
    "@vitest/coverage-v8": "^0.34.2",
    "dotenv": "^16.3.1",
    "happy-dom": "^10.11.0",
    "prettier": "^2.8.4",
    "rimraf": "^3.0.2",
    "rollup": "^3.28.1",
    "typescript": "^4.9.5",
    "vite": "^4.4.9",
    "vitest": "^0.34.3",
    "vitest-mock-extended": "^1.2.0"
  }
