{
  "name": "angular-sdk",
  "version": "0.0.0",
  "packageManager": "yarn@3.2.4",
  "scripts": {
    "ng": "ng",
    "start": "rm -rf dist && ng build --project video-angular-sdk --watch --configuration development & (wait-on dist && ng serve sample-app --host 0.0.0.0)",
    "build": "ng build --project video-angular-sdk && ng build --project sample-app",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "generate-docs": "typedoc --options typedoc.json --exclude '!**/*(stream-video.module|video.service).ts' ./projects/video-angular-sdk/src/public-api.ts && replace-in-file '# Class: ' '# ' temp-docs/** && cp -r temp-docs/classes/ generated-docs && rm -r temp-docs",
    "copy-docs": "cp -r ../client/generated-docs/ docusaurus/docs/Angular/reference && cp -r ./generated-docs/ docusaurus/docs/Angular/reference"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@popperjs/core": "^2.11.6",
    "@stream-io/video-client": "workspace:^",
    "@stream-io/video-styling": "workspace:^",
    "ngx-popperjs": "^14.1.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.3",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "~14.1.3",
    "@angular/compiler-cli": "^14.1.0",
    "@types/jasmine": "~4.0.0",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "eslint": "^8.23.1",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^14.1.0",
    "replace-in-file": "^6.3.5",
    "typedoc": "^0.23.16",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "~4.7.2",
    "wait-on": "^6.0.1"
  }
}
