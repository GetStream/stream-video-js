    "@stream-io/video-styling": "workspace:^",
    "autoprefixer": "^10.4.13",
    "classnames": "^2.3.2",
    "mapbox-gl": "^2.12.0",
    "postcss": "^8.4.21",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stream-chat": "^8.1.3",
    "stream-chat-react": "^10.4.2",
    "tailwindcss": "^3.2.4",
