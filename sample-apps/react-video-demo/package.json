    "@storybook/testing-library": "^0.0.13",
    "@types/babel__core": "^7",
    "@types/mapbox-gl": "^2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/uuid": "^8.3.4",
