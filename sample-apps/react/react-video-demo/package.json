    "@storybook/builder-vite": "^0.2.7",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@types/babel__core": "^7",
    "@types/geojson": "^7946.0.10",
    "@types/mapbox-gl": "^2.7.16",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@types/react-form": "^4",
    "@types/react-gtm-module": "^2",
